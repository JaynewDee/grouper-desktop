{
  "build": {
    "devPath": "http://localhost:1420",
    "distDir": "../build",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "GrouperDesktop",
    "version": "0.0.0"
  },

  "tauri": {
    "allowlist": {
      "window": {
        "all": true
      },
      "all": true,
      "shell": {
        "all": false,
        "open": true
      },
      "dialog": {
        "all": true,
        "open": true,
        "save": true
      },
      "fs": {
        "all": true,
        "scope": ["$APP/*", "$DESKTOP/*"]
      },
      "path": {
        "all": true
      }
    },
    "bundle": {
      "active": true,
      "category": "DeveloperTool",
      "copyright": "",
      "deb": {
        "depends": []
      },
      "externalBin": [],
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ],
      "identifier": "com.tauri.dev.grouper",
      "longDescription": "",
      "macOS": {
        "entitlements": null,
        "exceptionDomain": "",
        "frameworks": [],
        "providerShortName": null,
        "signingIdentity": null
      },
      "resources": [],
      "shortDescription": "",
      "targets": "all",
      "windows": {
        "certificateThumbprint": null,
        "digestAlgorithm": "sha256",
        "timestampUrl": ""
      }
    },
    "security": {
      "csp": null
    },
    "updater": {
      "active": false
    },
    "windows": [
      {
        "fullscreen": false,
        "height": 800,
        "maxHeight": 1080,
        "resizable": true,
        "title": "Grouper",
        "width": 600,
        "maxWidth": 1920,
        "decorations": true,
        "center": true
      }
    ]
  }
}
